    <page name="page">
        <triples>
            <!-- a label -->
            <triple s="http://wiktionary.dbpedia.org/resource/uri($entityId)" p="http://www.w3.org/2000/01/rdf-schema#label" o="$entityId" oType="literal"/>
            <!-- a link to the wiktionary page -->
            <triple s="http://wiktionary.dbpedia.org/resource/uri($entityId)" p="http://www.w3.org/2000/01/rdf-schema#seeAlso" o="http://en.wiktionary.org/wiki/uri($entityId)" oType="URI"/>
            <!-- provenance: list of authors -->
            <triple s="http://wiktionary.dbpedia.org/resource/uri($entityId)" p="http://purl.org/dc/elements/1.1/creator" o="http://en.wiktionary.org/w/index.php?title=uri($entityId)&amp;action=history" oType="URI"/>
            <!-- a type declaration -->
            <triple s="http://wiktionary.dbpedia.org/resource/uri($entityId)" p="http://www.w3.org/1999/02/22-rdf-syntax-ns#type" o="http://wiktionary.dbpedia.org/terms/LexicalEntity" oType="URI"/>
        </triples>
        <prologs>
            <template name="redirect">
                <resultTemplates>
                    <resultTemplate>
                        <triples>
                            <triple s="$block" p="http://www.w3.org/2002/07/owl#sameAs" o="http://wiktionary.dbpedia.org/resource/uri($otherPage)" oType="URI"/>
                        </triples>
                    </resultTemplate>
                </resultTemplates>
                <wikiTemplate>#REDIRECT ~~InternalLinkNode|$word|$otherPage~~</wikiTemplate>
            </template>
            <template name="also">
                <resultTemplates>
                            <resultTemplate>
                                <triples>
                                    <triple s="$block" p="http://www.w3.org/2000/01/rdf-schema#seeAlso" o="http://wiktionary.dbpedia.org/resource/uri($other)" oType="URI"/>
                               </triples>
                            
                            </resultTemplate>
                </resultTemplates>
                <wikiTemplate>{{also|$other}}</wikiTemplate>
            </template>
            <template name="wikipedia2">
                <resultTemplates>
                    <resultTemplate>
                        <triples>
                            <triple s="$block" p="http://www.w3.org/2000/01/rdf-schema#seeAlso" o="http://en.wikipedia.org/wiki/uri($word)" oType="URI"/>
                            <triple s="$block" p="http://www.w3.org/2002/07/owl#sameAs" o="http://dbpedia.org/resource/uri($word)" oType="URI"/>
                        </triples>
                    </resultTemplate>
                 </resultTemplates>
                 <wikiTemplate>{{wikipedia|$word}}</wikiTemplate>
            </template>
        </prologs>
        <epilogs>
            <template name="otherLanguages">
                <resultTemplates>
                    <resultTemplate>
                        <triples>
                            <triple s="$block" p="http://www.w3.org/2000/01/rdf-schema#seeAlso" o="http://$lang.wiktionary.org/wiki/uri($otherPage)" type="resource" />
                        </triples>
                    </resultTemplate>
                </resultTemplates>
                <wikiTemplate>
(~~InterWikiLinkNode|$lang:$otherPage|$label~~
)*</wikiTemplate>
            </template>
        </epilogs>
        <block name="lang" property="http://wiktionary.dbpedia.org/terms/hasLangUsage" >
            <indicators>
                <indicator name="lang-start">
                    <resultTemplates>
                        <resultTemplate>
                            <triples>
                                <triple s="$block" p="http://wiktionary.dbpedia.org/terms/hasLangUsage" o="$block-uri(map(assertMapped($language)))" oType="URI" oNewBlock="true" />
                                <triple s="$block" p="http://purl.org/dc/elements/1.1/language" o="http://wiktionary.dbpedia.org/terms/uri(map($language))" oType="URI"/>
                            </triples>
                        </resultTemplate>
                    </resultTemplates>
                    <wikiTemplate>{{-$language-}}
</wikiTemplate>
                </indicator>
            </indicators>
