            <triple s="http://wiktionary.dbpedia.org/resource/uri($entityId)" p="http://www.w3.org/2000/01/rdf-schema#seeAlso" o="http://de.wiktionary.org/wiki/uri($entityId)" oType="URI"/>
            <!-- provenance: list of authors -->
            <triple s="http://wiktionary.dbpedia.org/resource/uri($entityId)" p="http://purl.org/dc/elements/1.1/creator" o="http://de.wiktionary.org/w/index.php?title=uri($entityId)&amp;action=history" oType="URI"/>
