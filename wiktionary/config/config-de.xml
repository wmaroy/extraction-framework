                            </resultTemplate>
                            <resultTemplate>
                                <triples>
                                    <triple s="$block" p="http://wiktionary.dbpedia.org/terms/hasPronunciationSingular" o="$pronunciationSingular" oType="literal" oLang="de-fonipa"/>
