                                    <triple s="$block-clean($meaningId)de" p="http://www.w3.org/1999/02/22-rdf-syntax-ns#type" o="http://wiktionary.dbpedia.org/terms/Sense" oType="URI"/>
                                    <triple s="$block-clean($meaningId)de" p="http://wiktionary.dbpedia.org/terms/hasMeaning" o="$meaning" oType="literal" />
                                </triples>
                            </resultTemplate>
                        </resultTemplates>
                        <wikiTemplate>{{Bedeutungen}}
(:[$meaningId] $meaning
)+
</wikiTemplate>
                    </template>
                    <template name="etymology2">
